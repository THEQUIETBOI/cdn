        out vec3 vray_dir;

        mat4 scaling;

        void main() {


            vec3 pcopy = vec3(position.x, position.y, position.z);
            vec3 dif = pcopy - u_eye_pos;
            vray_dir = dif;
           
            vec4 outpos = projectionMatrix * viewMatrix * modelMatrix * vec4(position, 1.0);

